{
  "name": "unrouted-monorepo",
  "private": true,
  "version": "0.3.1",
  "description": "Minimal, composable Expressive Universal Router",
  "repository": "harlan-zw/unrouted",
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm -r --parallel run build",
    "stub": "pnpm -r --parallel run stub",
    "lint": "eslint \"{packages,test}/**/*.{ts,vue,json,yml}\"",
    "lint:fix": "npm run lint -- --fix",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag",
    "test": "vitest",
    "test:update": "vitest -u"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.16.0",
    "@types/express": "latest",
    "@types/koa": "^2.13.4",
    "@types/node": "17.0.12",
    "@unrouted/test-kit": "workspace:*",
    "bumpp": "^7.1.1",
    "connect": "latest",
    "eslint": "^8.7.0",
    "express": "^4.17.2",
    "h3": "^0.3.9",
    "koa": "^2.13.4",
    "listhen": "0.2.6",
    "supertest": "^6.2.2",
    "typescript": "^4.5.5",
    "unbuild": "^0.6.8",
    "vitest": "^0.2.4"
  }
}
