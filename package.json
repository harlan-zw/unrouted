{
  "name": "@unrouted/monorepo",
  "type": "module",
  "version": "0.5.0",
  "private": true,
  "packageManager": "pnpm@8.15.4",
  "scripts": {
    "build:packages": "pnpm --filter=./packages/** run build",
    "build:integrations": "pnpm --filter=./integrations/** run build",
    "build": "pnpm build:packages",
    "stub": "pnpm -r --parallel --filter=./packages/** run stub && pnpm -r --parallel --filter=./integrations/** run stub",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag",
    "test": "vitest",
    "test:update": "vitest -u"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@types/express": "^4.17.21",
    "@types/koa": "^2.15.0",
    "@types/node": "18.11.9",
    "@unrouted/test-kit": "workspace:*",
    "@vitest/ui": "^1.3.1",
    "bumpp": "^9.3.0",
    "connect": "latest",
    "eslint": "^8.57.0",
    "express": "^4.18.3",
    "h3": "^1.11.1",
    "koa": "^2.15.0",
    "listhen": "1.0.0",
    "supertest": "^6.3.4",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "unstorage": "^1.10.1",
    "vitest": "^1.3.1"
  }
}
