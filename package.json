{
  "name": "unrouted-monorepo",
  "private": true,
  "version": "0.3.4",
  "description": "Minimal, composable Expressive Universal Router",
  "repository": "harlan-zw/unrouted",
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build:packages": "pnpm -r --filter ./packages run build",
    "build:integrations": "pnpm -r --parallel --filter ./integrations run build",
    "build": "pnpm -r build:packages && pnpm -r --filter ./integrations run stub",
    "stub": "pnpm -r --parallel --filter ./packages run stub && pnpm -r --parallel --filter ./integrations run stub",
    "lint": "eslint \"{packages,test,integrations}/**/*.{ts,vue,json,yml}\"",
    "lint:fix": "npm run lint -- --fix",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag",
    "test": "vitest",
    "test:update": "vitest -u"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.18.9",
    "@types/express": "latest",
    "@types/koa": "^2.13.4",
    "@types/node": "17.0.21",
    "@unrouted/test-kit": "workspace:*",
    "@vitest/ui": "^0.7.6",
    "bumpp": "^7.1.1",
    "connect": "latest",
    "eslint": "^8.11.0",
    "express": "^4.17.3",
    "h3": "^0.4.2",
    "koa": "^2.13.4",
    "listhen": "0.2.6",
    "supertest": "^6.2.2",
    "typescript": "^4.6.2",
    "unbuild": "^0.7.0",
    "vitest": "^0.7.6"
  }
}
